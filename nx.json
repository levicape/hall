{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "defaultBase": "main",
  "useDaemonProcess": false,
  "neverConnectToCloud": true,
  "targetDefaults": {
    "build:tsc": {
      "cache": false,
      "outputs": [
        "{projectRoot}/commonjs",
        "{projectRoot}/module"
      ]
    },
    "build:esbuild": {
      "cache": false,
      "outputs": [
        "{projectRoot}/output/esbuild"
      ]
    },
    "http:build": {
      "cache": false,
      "outputs": [
        "{projectRoot}/output/http"
      ]
    },
    "staticwww:build": {
      "cache": false,
      "outputs": [
        "{projectRoot}/output/staticwww",
        "{projectRoot}/dist"
      ]
    },    
    "serverwww:build": {
      "cache": false,
      "outputs": [
        "{projectRoot}/output/serverwww",
        "{projectRoot}/dist"
      ]
    }
  }
}