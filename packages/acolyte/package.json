    "@cloudscape-design/global-styles": "^1.0.32",
    "@conform-to/react": "^1.2.2",
    "@conform-to/zod": "^1.2.2",
    "@epic-web/cachified": "^5.2.0",
    "@epic-web/client-hints": "^1.3.5",
    "@epic-web/invariant": "^1.0.0",
    "@epic-web/remember": "^1.1.0",
    "@epic-web/totp": "^2.0.0",
    "@paralleldrive/cuid2": "^2.2.2",
    "@shopify/polaris-icons": "^9.3.0",
    "@tanstack/react-form": "^0.34.4",
    "@tanstack/react-query": "^5.61.5",
    "@tanstack/react-query-devtools": "^5.61.5",
    "@tanstack/react-router": "^1.84.4",
    "@tanstack/react-table": "^8.20.5",
