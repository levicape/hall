{
  "name": "acolyte",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./tsc.out && rm -rf ./build && rm -rf .output && rm -rf .vinxi",
		"clean:install": "echo \"Clean node_modules if npm\"",
    "dev": "vinxi dev --port 5555",
    "prebuild": "pnpm run prepare:cloudscape",
    "build:node": "vinxi build --preset node-server && pnpm run resolve",
    "build:lambda": "vinxi build --preset aws-lambda && pnpm run resolve",
    "resolve": "pnpm run resolve:tslib && pnpm run resolve:clsx && pnpm run resolve:react-dom && pnpm run resolve:cookie",
    "resolve:tslib": "rm -rf .output/server/node_modules/tslib && cp -rf ./node_modules/tslib .output/server/node_modules/tslib",
    "resolve:clsx": "rm -rf .output/server/node_modules/clsx && cp -r ./node_modules/clsx .output/server/node_modules",
    "resolve:react-dom": "rm -rf .output/server/node_modules/react-dom && cp -r ./node_modules/react-dom .output/server/node_modules",
    "resolve:cookie": "rm -rf .output/server/node_modules/cookie && cp -r ./node_modules/cookie .output/server/node_modules",
    "start:node": "vinxi start",
    "start:lambda": "node .output/server/index.mjs",
    "lint": "npx @biomejs/biome check --write ./app ./server"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@tanstack/router-plugin": "^1.84.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.12",
    "@types/cookie-signature": "^1.1.2",
    "@types/node": "^22.10.1",
    "@types/qrcode": "^1.5.5",
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "node-gyp": "^10.2.0",
    "postcss": "^8.4.49",
    "sass-embedded": "^1.81.0",
    "tailwindcss": "^3.4.15",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-plugin-cjs-interop": "^2.1.5",
    "vite-plugin-esmodule": "^1.5.0",
    "vite-tsconfig-paths": "^4.3.2",
    "webpack": "^5.96.1"
  },
  "overrides": {
    "@types/react": "npm:types-react@rc",
    "@types/react-dom": "npm:types-react-dom@rc"
  },
  "peerDependencies": {
    "typescript": "^5.7.2"
  },
  "pnpm": {
    "overrides": {
      "react-keyed-flatten-children": "$react-keyed-flatten-children"
    }
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.2",
