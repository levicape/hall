    "@tanstack/start": "^1.84.4",
    "@tanstack/zod-form-adapter": "^0.34.4",
    "@total-typescript/ts-reset": "^0.6.1",
    "@uidotdev/usehooks": "^2.4.1",
    "ahooks": "^3.8.1",
    "aws4fetch": "^1.0.20",
    "bcryptjs": "^2.4.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cookie": "^1.0.2",
    "cookie-signature": "^1.2.2",
    "crypto-js": "^4.2.0",
    "crypto.js": "^3.2.2",
    "http-status-codes": "^2.3.0",
    "input-otp": "^1.4.1",
    "isbot": "^4.4.0",
    "jsonwebtoken": "^9.0.2",
    "ksuid": "^3.0.0",
    "long": "^5.2.3",
    "lru-cache": "^11.0.2",
    "node-html-parser": "^6.1.13",
